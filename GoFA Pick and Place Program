! This program was another experiment in iterating across targets. After learning the object structure of robot targets in RAPID, 
! I essentially created 8 iterables for 8 pallets, used 4 template points at the corner of one pallet, and translated them by a certain amount based on the arrangement 
! and geometry of each pallet. This program also has signal commands for a LinearMover smart component in RobotStudio Station logic, as the station managed
! two trays of the same type of components which would be exchanged when one tray was exhausted. This program did not face many configuration issues, as most targets were comfortably
! within the robotic work envelope, and were a safe distance away from the edges of it's dextrous region to prevent problematic configuration reversals. 

MODULE Module1
    CONST robtarget Home:=[[1326.379951193,-60.744304613,306.76242246],[0,0,1,0],[0,0,0,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget pallet1:=[[1323.15350694,-210.82702527,137.028292252],[0,0,1,0],[-1,-1,-1,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget pallet2:=[[1323.15350694,-29.514072999,137.028292252],[0,0,1,0],[-1,-1,-1,1],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget pallet3:=[[1484.296065261,-29.514072999,137.028292252],[0,0,1,0],[-1,-1,-1,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget pallet4:=[[1484.296065261,-211.242731742,137.028292252],[0,0,1,0],[-1,-1,-1,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget Target_10:=[[1106.297476912,-208.712670826,172.843804882],[0,-0.707106781,0.707106781,0],[0,0,0,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget Target_20:=[[723.412595846,935.141937069,218.593524498],[0,-0.707106781,0.707106781,0],[0,0,0,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget Target_20_2:=[[663.49665337,935.141937069,218.593524498],[0,-0.707106781,0.707106781,0],[0,0,0,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    VAR robtarget p1{4}:=[pallet1,pallet2,pallet3,pallet4];
    VAR robtarget p2{4};
    VAR robtarget p3{4};
    VAR robtarget p4{4};
    VAR robtarget p5{4};
    VAR robtarget p6{4};
    VAR robtarget p7{4};
    VAR robtarget p8{4};
    
    CONST robtarget pallet2_2:=[[1119.014745607,-631.584609498,172.843804882],[0,0,1,0],[-1,2,1,1],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget pallet1_2:=[[1323.15350694,61.17297473,137.028292252],[0,0,1,0],[-1,-1,-1,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget pallet1_2_2:=[[1323.15350694,61.17297473,287.028292252],[0,0,1,0],[-1,-1,-1,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget pallet2_3:=[[1323.15350694,242.485927001,137.028292252],[0,0,1,0],[-1,-1,-1,1],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget pallet2_3_2:=[[1323.15350694,242.485927001,287.028292252],[0,0,1,0],[0,0,0,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget pallet3_2:=[[1484.296065261,242.485927001,137.028292252],[0,0,1,0],[-1,-1,-1,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget pallet3_2_2:=[[1484.296065261,242.485927001,287.028292252],[0,0,1,0],[-1,-1,-1,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget pallet4_2:=[[1484.296065261,60.757268258,137.028292252],[0,0,1,0],[-1,-1,-1,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    CONST robtarget pallet4_2_2:=[[1484.296065261,60.757268258,287.028292252],[0,0,1,0],[-1,-1,-1,0],[9E+09,9E+09,9E+09,9E+09,9E+09,9E+09]];
    
