    PROC PnPpath()
    ENDPROC
    PROC generatezoffs()
        VAR robtarget temp;
        FOR i FROM 1 TO 4 DO
            temp:=p1{i};
            temp.trans.x:=temp.trans.x-272;
            temp.trans.z:=temp.trans.z+331;
            p2{i}:=temp;
        ENDFOR
        FOR i FROM 1 TO 4 DO
            temp:=p2{i};
            temp.trans.y:=temp.trans.y+211;
            temp.trans.z:=temp.trans.z+331;
            p3{i}:=temp;
            temp:=p3{i};
            temp.trans.y:=temp.trans.y+211;
            temp.trans.z:=temp.trans.z+331;
            p4{i}:=temp;
            temp:=p4{i};
            temp.trans.y:=temp.trans.y+211;
            temp.trans.z:=temp.trans.z+331;
            p5{i}:=temp;
            
            temp:=p5{i};
            temp.trans.x:=temp.trans.x+272;
            temp.trans.z:=temp.trans.z+331;
            p6{i}:=temp;
            
            temp:=p6{i};
            temp.trans.y:=temp.trans.y-211;
            temp.trans.z:=temp.trans.z+331;
            p7{i}:=temp;
            
            temp:=p7{i};
            temp.trans.y:=temp.trans.y-211;
            temp.trans.z:=temp.trans.z+331;
            p8{i}:=temp;
        ENDFOR
    ENDPROC
    PROC generatearrays()
        VAR robtarget temp;
        FOR i FROM 1 TO 4 DO
            temp:=p1{i};
            temp.trans.y:=temp.trans.y+272;
            p2{i}:=temp;
        ENDFOR
        FOR i FROM 1 TO 4 DO
            temp:=p2{i};
            temp.trans.x:=temp.trans.x+211;
            p3{i}:=temp;
            temp:=p3{i};
            temp.trans.x:=temp.trans.x+211;
            p4{i}:=temp;
            temp:=p4{i};
            temp.trans.x:=temp.trans.x+211;
            p5{i}:=temp;
            
            temp:=p5{i};
            temp.trans.y:=temp.trans.y-272;
            p6{i}:=temp;
            
            temp:=p6{i};
            temp.trans.x:=temp.trans.x-211;
            p7{i}:=temp;
            
            temp:=p7{i};
            temp.trans.x:=temp.trans.x-211;
            p8{i}:=temp;
        ENDFOR
        FOR i FROM 1 TO 4 DO
            p2{i}.robconf:=p1{i}.robconf;
            p3{i}.robconf:=p1{i}.robconf;
            p4{i}.robconf:=p1{i}.robconf;
            p5{i}.robconf:=p1{i}.robconf;
            p6{i}.robconf:=p1{i}.robconf;
            p7{i}.robconf:=p1{i}.robconf;
            p8{i}.robconf:=p1{i}.robconf;
        ENDFOR
    ENDPROC
    !8,1
    !3,2
    !5,4
    !6,7
    PROC Tes()
        generatearrays;
        FOR i FROM 1 TO 4 DO
            MoveJ p1{i},v1000,fine,Tooldata_1\WObj:=wobj0;
            MoveJ p2{i},v1000,fine,Tooldata_1\WObj:=wobj0;
            MoveJ p3{i},v1000,fine,Tooldata_1\WObj:=wobj0;
            MoveJ p4{i},v1000,fine,Tooldata_1\WObj:=wobj0;
            MoveJ p5{i},v1000,fine,Tooldata_1\WObj:=wobj0;
            MoveJ p6{i},v1000,fine,Tooldata_1\WObj:=wobj0;
            MoveJ p7{i},v1000,fine,Tooldata_1\WObj:=wobj0;
        ENDFOR
        FOR i FROM 1 TO 4 DO
            p1{i}.trans.z:=p1{i}.trans.z+300;
            MoveJ p1{i},v1000,fine,Tooldata_1\WObj:=wobj0;
            p2{i}.trans.z:=p2{i}.trans.z+300;
            MoveJ p2{i},v1000,fine,Tooldata_1\WObj:=wobj0;
            p3{i}.trans.z:=p3{i}.trans.z+300;
            MoveJ p3{i},v1000,fine,Tooldata_1\WObj:=wobj0;
            p4{i}.trans.z:=p4{i}.trans.z+300;
            MoveJ p4{i},v1000,fine,Tooldata_1\WObj:=wobj0;
            p5{i}.trans.z:=p5{i}.trans.z+300;
            MoveJ p5{i},v1000,fine,Tooldata_1\WObj:=wobj0;
            p6{i}.trans.z:=p6{i}.trans.z+300;
            MoveJ p6{i},v1000,fine,Tooldata_1\WObj:=wobj0;
            p7{i}.trans.z:=p7{i}.trans.z+300;
            MoveJ p7{i},v1000,fine,Tooldata_1\WObj:=wobj0;
            p8{i}.trans.z:=p8{i}.trans.z+300;
            MoveJ p8{i},v1000,fine,Tooldata_1\WObj:=wobj0;
        ENDFOR
        
        
        !MoveJ p8{i},v1000,fine,Tooldata_1\WObj:=wobj0;
    ENDPROC
    PROC main()
        VAR robtarget secpick;
        VAR robtarget a;
        generatearrays;
        WaitTime 0.1;
        SetDO Pick1,0;
        WaitTime 0.1;
        SetDO pick2,0;
        WaitTime 0.1;
        SetDO pick3,0;
        WaitTime 0.1;
        SetDO pick4,0;
        WaitTime 0.1;
        WaitTime 0.1;
        SetDO exchange,1;
        WaitTime 0.1;
        SetDO exchange,0;
        WaitTime 0.1;
        ! 8,1
        ! The following FOR loops iterate across a grid of 8 pallets arranged in 2 rows
        !4 targets are created on one pallet, then offset in the x and y directions dynamically by accessing the object definitions of each target
        FOR i FROM 1 TO 4 DO
            IF i=1 THEN
                 
                a:=target_20;
                a.robconf:=target_20.robconf;
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                WaitTime 0.2;
                SetDO Pick1,1;
                WaitTime 0.2;
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                 
                p1{i}.trans.z:=p1{i}.trans.z+150;
                MoveJ p1{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                p1{i}.trans.z:=p1{i}.trans.z-150;
                MoveJ p1{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                WaitTime 0.2;
                SetDO pick1,0;
                WaitTime 0.2;
                p1{i}.trans.z:=p1{i}.trans.z+150;
                MoveJ p1{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                p1{i}.trans.z:=p1{i}.trans.z-150;
                 
                a:=target_20_2;
                a.robconf:=target_20.robconf;
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                WaitTime 0.2;
                SetDO Pick2,1;
                WaitTime 0.2;
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                WaitTime 0.2;
                secpick:=p1{i};
                IF i=1 OR i=4 THEN
                    secpick.trans.y:=secpick.trans.y+60;
                ELSE
                    secpick.trans.y:=secpick.trans.y-60;
                ENDIF
                secpick.robconf:=p1{i}.robconf;
                secpick.trans.z:=secpick.trans.z+150;
                MoveJ secpick,v1000,fine,Tooldata_1\WObj:=wobj0;
                secpick.trans.z:=secpick.trans.z-150;
                MoveJ secpick,v1000,fine,Tooldata_1\WObj:=wobj0;
                WaitTime 0.2;
                SetDO pick2,0;
                WaitTime 0.2;
                secpick.trans.z:=secpick.trans.z+150;
                MoveJ secpick,v1000,fine,Tooldata_1\WObj:=wobj0;
                secpick.trans.z:=secpick.trans.z-150;
                 
                p8{i}.trans.z:=p8{i}.trans.z+150;
                MoveJ p8{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                p8{i}.trans.z:=p8{i}.trans.z-150;
                MoveJ p8{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                WaitTime 0.2;
                SetDO pick3,1;
                WaitTime 0.2;
                p8{i}.trans.z:=p8{i}.trans.z+150;
                MoveJ p8{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                p8{i}.trans.z:=p8{i}.trans.z-150;
                 
                a:=target_20_2;
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                WaitTime 0.2;
                SetDO Pick3,0;
                WaitTime 0.2;
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                 
                secpick:=p8{i};
                IF i=1 OR i=4 THEN            
                    secpick.trans.y:=secpick.trans.y+60;
                ELSE
                    secpick.trans.y:=secpick.trans.y-60;
                ENDIF
                secpick.robconf:=p8{i}.robconf;
                secpick.trans.z:=secpick.trans.z+150;
                MoveJ secpick,v1000,fine,Tooldata_1\WObj:=wobj0;
                secpick.trans.z:=secpick.trans.z-150;
                MoveJ secpick,v1000,fine,Tooldata_1\WObj:=wobj0;
                WaitTime 0.2;
                SetDO pick4,1;
                WaitTime 0.2;
                secpick.trans.z:=secpick.trans.z+150;
                MoveJ secpick,v1000,fine,Tooldata_1\WObj:=wobj0;
                secpick.trans.z:=secpick.trans.z-150;
                 
                a:=target_20_2;
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                WaitTime 0.2;
                SetDO Pick4,0;
                WaitTime 0.2;
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
            ELSE
                 
                a:=target_20;
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                 
                p1{i}.trans.z:=p1{i}.trans.z+150;
                MoveJ p1{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                p1{i}.trans.z:=p1{i}.trans.z-150;
                MoveJ p1{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                
                p1{i}.trans.z:=p1{i}.trans.z+150;
                MoveJ p1{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                p1{i}.trans.z:=p1{i}.trans.z-150;
                 
                a:=target_20_2;
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                WaitTime 0.2;
                secpick:=p1{i};
                IF i=1 OR i=4 THEN
                    secpick.trans.y:=secpick.trans.y+60;
                ELSE
                    secpick.trans.y:=secpick.trans.y-60;
                ENDIF
                 
                secpick.trans.z:=secpick.trans.z+150;
                MoveJ secpick,v1000,fine,Tooldata_1\WObj:=wobj0;
                secpick.trans.z:=secpick.trans.z-150;
                MoveJ secpick,v1000,fine,Tooldata_1\WObj:=wobj0;
                
                secpick.trans.z:=secpick.trans.z+150;
                MoveJ secpick,v1000,fine,Tooldata_1\WObj:=wobj0;
                secpick.trans.z:=secpick.trans.z-150;
                 
                p8{i}.trans.z:=p8{i}.trans.z+150;
                MoveJ p8{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                p8{i}.trans.z:=p8{i}.trans.z-150;
                MoveJ p8{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                
                p8{i}.trans.z:=p8{i}.trans.z+150;
                MoveJ p8{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                p8{i}.trans.z:=p8{i}.trans.z-150;
                 
                a:=target_20_2;
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                 
                secpick:=p8{i};
                IF i=1 OR i=4 THEN            
                    secpick.trans.y:=secpick.trans.y+60;
                ELSE
                    secpick.trans.y:=secpick.trans.y-60;
                ENDIF
                secpick.trans.z:=secpick.trans.z+150;
                MoveJ secpick,v1000,fine,Tooldata_1\WObj:=wobj0;
                secpick.trans.z:=secpick.trans.z-150;
                MoveJ secpick,v1000,fine,Tooldata_1\WObj:=wobj0;
                
                secpick.trans.z:=secpick.trans.z+150;
                MoveJ secpick,v1000,fine,Tooldata_1\WObj:=wobj0;
                secpick.trans.z:=secpick.trans.z-150;
                 
                a:=target_20_2;
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
            ENDIF
        ENDFOR
        
        FOR i FROM 1 TO 4 DO
                 
                a:=target_20;
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
              
