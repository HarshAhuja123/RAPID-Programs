                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                 
                p2{i}.trans.z:=p2{i}.trans.z+150;
                MoveJ p2{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                p2{i}.trans.z:=p2{i}.trans.z-150;
                MoveJ p2{i},v1000,fine,Tooldata_1\WObj:=wobj0;
             
                p2{i}.trans.z:=p2{i}.trans.z+150;
                MoveJ p2{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                p2{i}.trans.z:=p2{i}.trans.z-150;
                 
                a:=target_20_2;
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
               
                a.trans.z:=a.trans.z+150;
                MoveJ a,v1000,fine,Tooldata_1\WObj:=wobj0;
                a.trans.z:=a.trans.z-150;
                 
                WaitTime 0.2;
                secpick:=p2{i};
                IF i=1 OR i=4 THEN
                    secpick.trans.y:=secpick.trans.y+60;
                ELSE
                    secpick.trans.y:=secpick.trans.y-60;
                ENDIF
                secpick.trans.z:=secpick.trans.z+150;
                MoveJ secpick,v1000,fine,Tooldata_1\WObj:=wobj0;
                secpick.trans.z:=secpick.trans.z-150;
                MoveJ secpick,v1000,fine,Tooldata_1\WObj:=wobj0;
                
                secpick.trans.z:=secpick.trans.z+150;
                MoveJ secpick,v1000,fine,Tooldata_1\WObj:=wobj0;
                secpick.trans.z:=secpick.trans.z-150;
                 
                p3{i}.trans.z:=p3{i}.trans.z+150;
                MoveJ p3{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                p3{i}.trans.z:=p3{i}.trans.z-150;
                MoveJ p3{i},v1000,fine,Tooldata_1\WObj:=wobj0;
                
                p3{i}.trans.z:=p3{i}.trans.z+150;
                MoveJ p3{i},v1000,fine,Tooldata_1\WObj:=wobj0;
